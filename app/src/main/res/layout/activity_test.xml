<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.test">

    <Button
        android:id="@+id/btnCaregiver"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="161dp"
        android:layout_marginBottom="37dp"
        android:text="Caregiver"
        app:layout_constraintBottom_toTopOf="@+id/btnCarereceiver"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btnCarereceiver"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="43dp"
        android:text="Carereceiver"
        app:layout_constraintBottom_toTopOf="@+id/btnAdmin"
        app:layout_constraintEnd_toEndOf="@+id/btnCaregiver"
        app:layout_constraintStart_toStartOf="@+id/btnCaregiver" />

    <Button
        android:id="@+id/btnAdmin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="320dp"
        android:text="Admin"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/btnCarereceiver"
        app:layout_constraintStart_toStartOf="@+id/btnCarereceiver" />
</androidx.constraintlayout.widget.ConstraintLayout>